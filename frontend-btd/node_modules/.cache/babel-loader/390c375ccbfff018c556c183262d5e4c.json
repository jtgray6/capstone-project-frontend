AAK,EAALA;AAAF,OAAZ;AACH;AATM;AAhBM,CAAd","sourcesContent":["import { userService } from '../services';\n\nexport const users = {\n    namespaced: true,\n    state: {\n        all: {}\n    },\n    actions: {\n        getAll({ commit }) {\n            commit('getAllRequest');\n\n            userService.getAll()\n                .then(\n                    users => commit('getAllSuccess', users),\n                    error => commit('getAllFailure', error)\n                );\n        }\n    },\n    mutations: {\n        getAllRequest(state) {\n            state.all = { loading: true };\n        },\n        getAllSuccess(state, users) {\n            state.all = { items: users };\n        },\n        getAllFailure(state, error) {\n            state.all = { error };\n        }\n    }\n}"]}]}