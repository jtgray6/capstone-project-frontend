                  }\n                );\n        },\n        logout({ commit }) {\n            userService.logout();\n            commit('logout');\n        }\n    },\n    mutations: {\n        loginRequest(state, user) {\n            state.status = { loggingIn: true };\n            state.user = user;\n        },\n        loginSuccess(state, user) {\n            state.status = { loggedIn: true };\n            state.user = user;\n        },\n        loginFailure(state) {\n            state.status = {};\n            state.user = null;\n        },\n        logout(state) {\n            state.status = {};\n            state.user = null;\n        }\n    }\n}"]}]}